{
  "name": "mozg-tolyana-bot",
  "version": "0.0.0",
  "private": true,
  "author": "yratanov <organium@gmail.com>",
  "description": "A simple helpful robot for your Company",
  "dependencies": {
    "coffee-script": "^1.12.4",
    "hubot": "^2.19.0",
    "hubot-diagnostics": "0.0.2",
    "hubot-google-images": "^0.2.7",
    "hubot-google-translate": "^0.2.1",
    "hubot-help": "^0.2.2",
    "hubot-heroku-keepalive": "^1.0.3",
    "hubot-maps": "0.0.3",
    "hubot-pugme": "^0.1.1",
    "hubot-redis-brain": "0.0.4",
    "hubot-rules": "^0.1.2",
    "hubot-scripts": "^2.17.2",
    "hubot-shipit": "^0.2.1",
    "hubot-telegram": "^0.1.2"
  },
  "engines": {
    "node": "7.10.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ]
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
